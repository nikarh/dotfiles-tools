pkgname=wine-nvcuda
pkgver=7.20
pkgrel=1
pkgdesc="Nvcuda dll extracted from older version of wine"
arch=('i686' 'x86_64')
url="https://github.com/nikarh/dotfiles-tools/"
license=('MIT')
source=("https://archive.archlinux.org/repos/2022/11/26/multilib/os/x86_64/wine-staging-7.20-1-x86_64.pkg.tar.zst")
noextract=("wine-staging-7.20-1-x86_64.pkg.tar.zst")
sha256sums=('3c747a6e10cc7be2df1b7e94535b5b0b4d559fb7393491a5c50479d669ea514f')

package() {
  tar -xvf ./wine-staging-7.20-1-x86_64.pkg.tar.zst  --wildcards "*nvcuda*"
  cp -a $srcdir/usr "$pkgdir/usr"
  install -d $dstdir/usr
}
